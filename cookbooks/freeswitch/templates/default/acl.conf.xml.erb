<configuration name="acl.conf" description="Network Lists">
    <network-lists>
        <list name="trusted" default="deny">
          <!--  <node type="allow" cidr="64.136.174.35/32"/> -->
            <X-PRE-PROCESS cmd="include" data="/etc/freeswitch/carriers.xml"/>
        </list>
        <list name="authoritative" default="deny">
          <% @opensips.each do |fqdn, ipaddress| -%>
            <node type="allow" cidr="<%= ipaddress %>/32"/> <!--<%= fqdn %>-->
          <% end %>
        </list>
    </network-lists>
</configuration>
